<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="onj.board.dao.BoardDAO">

	<typeAlias  alias="UserVO" type="com.study.myhome.user.service.UserVO"/>
	<select id="UserDAO.selectUsers" parameterType="hashmap" resultType="UserVO">
	
		<![CDATA[
		SELECT 	USERNAME,
				PASSWORD,
				JOIN_DATE
			FROM USER
		]]>
	</select>

</mapper>
